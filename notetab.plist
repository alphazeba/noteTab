<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>

    <key>Label</key>
    <string>com.aronhomm.notetab</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StartInterval</key>
    <integer>20</integer>

    <key>StandardErrorPath</key>
    <string>/Users/arnhom/.notetab/logs/stderr.log</string>

    <key>StandardOutPath</key>
    <string>/Users/arnhom/.notetab/logs/stdout.log</string>

    <key>WorkingDirectory</key>
    <string>/Users/arnhom/Projects/noteTab</string>

    <key>ProgramArguments</key>
    <array>
      <string>/Users/arnhom/Projects/noteTab/run.sh</string>
    </array>

  </dict>
</plist>